{
  "extends": [
    "config:base",
    "group:allNonMajor",
    ":semanticCommitTypeAll(chore)"
  ],
  "packageRules": [
    {
      "depTypeList": [
        "peerDependencies"
      ],
      "enabled": false
    }
  ],
  "labels": ["renovate"],
  "rangeStrategy": "bump",
  "lockFileMaintenance": {
    "enabled": true
  }  
}
